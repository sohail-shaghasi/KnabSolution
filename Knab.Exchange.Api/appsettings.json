{
  "ServiceConfigurations": {
    "Environment": "LocalDevelopment",
    "ServiceInfo": {
      "Name": "Knab.Exchange",
      "RepositoryUrl": ""
    },
    "CoinmarketcapApi": {
      "baseUrl": "https://pro-api.coinmarketcap.com",
      "Version": "v2",
      "QuotesEndpoint": "cryptocurrency/quotes/latest",
      "MapEndpoint": "cryptocurrency/map",
      "APIKeyName": "X-CMC_PRO_API_KEY",
      "APIKeyValue": "77756464-e009-4f26-9d95-966f54b65338",
      "DefaultTargetedCurrencies": [ "USD" ],
      "SupportedTargetedCurrencies": [ "USD", "GBP", "EUR", "BRL", "AUD" ]
    },
    "Exchangeratesapi": {
      "ServiceBaseUrl": "https://api.exchangeratesapi.io",
      "ExchangeRateEndpoint": "latest",
      "SupportedCurrencies": [ "USD", "GBP", "EUR", "BRL", "AUD", "JPY", "NOK", "SGD" ],
      "DefaultTargetedCurrencies": [ "USD", "GBP", "EUR", "BRL", "AUD" ],
      "EnableCaching": true,
      "ExpiredAfterInMinutes": 120
    },
    "LoggingSinks": {
      "ApplicationInsights": {
        "Enabled": false
      },
      "Console": {
        "LogLevel": {
          "Default": "Verbose",
          "System": "Debug",
          "Microsoft": "Debug"
        },
        "Enabled": true,
        "OutputTemplate": "{TimeStampUtc:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] [{AppName}] {Message:l}{NewLine}{Exception}"
      },
      "RollingFile": {
        "Enabled": true,
        "Location": "_Logs",
        "Extension": "log",
        "OutputTemplate": "{TimeStampUtc:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] [{AppName}] {Message:l}{NewLine}{Exception}",
        "RollingInterval": "Day",
        "MimimumLevel": "Information"
      }
    },
    "Logging": {
      "MimimumLevel": "Information",
      "LogLevel": {
        "Default": "Verbose",
        "System": "Debug",
        "Microsoft": "Debug"
      }
    }
  }
}
